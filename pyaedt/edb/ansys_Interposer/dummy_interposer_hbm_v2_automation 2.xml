  <SimulationSetups>
		<HFSSSetup schemaVersion="1.0" Name="HFSS 5GHz">
			<HFSSSimulationSettings>
				<HFSSAdaptiveSettings>
					<AdaptiveSettings>
						<SingleFrequencyDataList>
							<AdaptiveFrequencyData>
								<AdaptiveFrequency>5GHz</AdaptiveFrequency>
								<MaxPasses>99</MaxPasses>
								<MaxDelta>0.02</MaxDelta>
							</AdaptiveFrequencyData>
						</SingleFrequencyDataList>
					</AdaptiveSettings>
				</HFSSAdaptiveSettings>
				<HFSSDefeatureSettings>
					<ModelType>IC</ModelType>
					<ICModelResolutionType>Auto</ICModelResolutionType>
				</HFSSDefeatureSettings>
				<HFSSModellingSettings>
					<IntraPlaneCoupling>True</IntraPlaneCoupling>
				</HFSSModellingSettings>
				<HFSSSolverSettings>
					<OrderBasis>FirstOrder</OrderBasis>
					<!-- <SolverType>Auto</SolverType> -->
					<!-- <EnhancedLowFreqAccuracy>True</EnhancedLowFreqAccuracy> -->
				</HFSSSolverSettings>
				<HFSSMeshOperations>
					<MeshOperation>
						<Name>DQ_Seeding</Name>
						<Enabled>true</Enabled>
						<RefineInside>false</RefineInside>
						<Region>On Selection</Region>
						<Type>MeshOperationLength</Type>
						<NetsLayers>
							<NetLayer>
								<Net>DQ*</Net>
								<Layer>metal3</Layer>
							</NetLayer>
							<NetLayer>
								<Net>DQ*</Net>
								<Layer>metal5</Layer>
							</NetLayer>
						</NetsLayers>
						<RestrictElem>false</RestrictElem>
						<NumMaxElem>1000</NumMaxElem>
						<RestrictLength>true</RestrictLength>
						<MaxLength>15um</MaxLength>
					</MeshOperation>
				</HFSSMeshOperations>
			</HFSSSimulationSettings>
			<HFSSSweepDataList>
				<FreqSweep>
					<Name>Sweep1</Name>
					<UseQ3DForDC>True</UseQ3DForDC>
					<Interpolating>
						<LinearStep>
							<Start>0Hz</Start>
							<Stop>10GHz</Stop>
							<Step>50MHz</Step>
						</LinearStep>
					</Interpolating>
				</FreqSweep>				
			</HFSSSweepDataList>
		</HFSSSetup>
  </SimulationSetups>

